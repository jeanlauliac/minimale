{
  "command_line_templates": [
    {
      "binary_path": "/usr/bin/flex",
      "arguments": [
        {
          "literals": [
            "-o"
          ],
          "variables": [
            "output_file",
            "input_files"
          ]
        }
      ]
    },
    {
      "binary_path": "/usr/bin/bison",
      "arguments": [
        {
          "literals": [
            "-do"
          ],
          "variables": [
            "output_file",
            "input_files"
          ]
        }
      ]
    },
    {
      "binary_path": "clang++",
      "arguments": [
        {
          "literals": [
            "-c",
            "-o"
          ],
          "variables": [
            "output_file"
          ]
        },
        {
          "literals": [
            "-std=c++14",
            "-Wall",
            "-fcolor-diagnostics",
            "-MMD",
            "-MF"
          ],
          "variables": [
            "dependency_file"
          ]
        },
        {
          "literals": [
            "-I",
            "/usr/local/include"
          ],
          "variables": [
            "input_files"
          ]
        }
      ]
    },
    {
      "binary_path": "clang++",
      "arguments": [
        {
          "literals": [
            "-c",
            "-o"
          ],
          "variables": [
            "output_file"
          ]
        },
        {
          "literals": [
            "-std=c++14",
            "-Wall",
            "-Wno-deprecated-register",
            "-Wno-unused-function",
            "-Wno-unneeded-internal-declaration",
            "-fcolor-diagnostics",
            "-MMD",
            "-MF"
          ],
          "variables": [
            "dependency_file"
          ]
        },
        {
          "literals": [
            "-I",
            "/usr/local/include"
          ],
          "variables": [
            "input_files"
          ]
        }
      ]
    },
    {
      "binary_path": "clang++",
      "arguments": [
        {
          "literals": [
            "-o"
          ],
          "variables": [
            "output_file"
          ]
        },
        {
          "literals": [
            "-Wall",
            "-std=c++14",
            "-fcolor-diagnostics",
            "-L",
            "/usr/local/lib"
          ],
          "variables": [
            "input_files"
          ]
        }
      ]
    }
  ],
  "source_patterns": [
    "(src/**/*.lx)",
    "(src/**/*.bs)",
    "(src/**/*).cpp"
  ],
  "rules": [
    {
      "dependencies": [],
      "command_line_ix": 0,
      "inputs": [
        {
          "source_ix": 0
        }
      ],
      "output": ".build_files/($1).cpp"
    },
    {
      "dependencies": [],
      "command_line_ix": 1,
      "inputs": [
        {
          "source_ix": 1
        }
      ],
      "output": ".build_files/($1).cpp"
    },
    {
      "dependencies": [
        {
          "rule_ix": 1
        }
      ],
      "command_line_ix": 2,
      "inputs": [
        {
          "source_ix": 2
        }
      ],
      "output": ".build_files/($1).o"
    },
    {
      "dependencies": [],
      "command_line_ix": 3,
      "inputs": [
        {
          "rule_ix": 0
        }
      ],
      "output": ".build_files/($1).o"
    },
    {
      "dependencies": [],
      "command_line_ix": 3,
      "inputs": [
        {
          "rule_ix": 1
        }
      ],
      "output": ".build_files/($1).o"
    },
    {
      "dependencies": [],
      "command_line_ix": 4,
      "inputs": [
        {
          "rule_ix": 2
        },
        {
          "rule_ix": 3
        },
        {
          "rule_ix": 4
        }
      ],
      "output": "dist/minimale"
    }
  ]
}